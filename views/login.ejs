</style>

<div class= "text-center  bg-primary p-3">
    <a class="text-dark m-4 " href="/" > Entrar </a>
    </div>
  
    <div class="container" >
         <form method="post" >
    
          <h1 class="h3 mb-3 fw-normal"> Identifique-se. </h1>
          <div class="form-floating">
           
            <input name="username" type="text" class="form-control m-2" id="floatingInput" placeholder="Usuário">
            <label for="inputUsername">Usuário</label>
          </div>  
  
  
          <div class="form-floating">
            <input name="email" type="email" class="form-control m-2" id="inputEmail" placeholder="email">
            <label for="inputEmail">Email</label>
          </div>
          <div class="form-floating">
            <input name="password" type="password" class="form-control m-2" id="inputPassword" placeholder="password">
            <label for="inputPassword">Password</label>
          </div>
      
          <div class="checkbox mb-3">
            
          </div>
          <button class="w-100 btn btn-lg btn-primary" type="submit">Entrar</button>
          <p class="mt-5 mb-3 text-muted">&copy; 2017–2021</p>
        </form>
    </div>
  
  
  
  
